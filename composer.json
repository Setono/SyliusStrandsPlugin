{
    "name": "setono/sylius-strands-plugin",
    "type": "sylius-plugin",
    "description": "Plugin that integrates Strands",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "setono/tag-bag-bundle": "^2.0",
        "setono/tag-bag-twig": "^1.0",
        "sylius/sylius": "^1.4"
    },
    "require-dev": {
        "matthiasnoback/symfony-config-test": "^4.2",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1",
        "phpspec/phpspec": "^6.2",
        "phpunit/phpunit": "^9.2",
        "roave/security-advisories": "dev-master",
        "setono/code-quality-pack": "^2.3",
        "setono/sylius-tag-bag-plugin": "^1.1",
        "symfony/debug-bundle": "^5.1",
        "symfony/dotenv": "^5.1",
        "symfony/intl": "^4.4 || ^5.1",
        "symfony/web-profiler-bundle": "^5.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\SyliusStrandsPlugin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Setono\\SyliusStrandsPlugin\\": "tests/"
        },
        "classmap": [
            "tests/Application/Kernel.php"
        ]
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max src",
        "check-style": "vendor/bin/ecs check src",
        "fix-style": "vendor/bin/ecs check src --fix",
        "phpspec": "vendor/bin/phpspec run",
        "phpunit": "vendor/bin/phpunit",
        "test": [
            "@phpspec",
            "@phpunit"
        ]
    }
}
